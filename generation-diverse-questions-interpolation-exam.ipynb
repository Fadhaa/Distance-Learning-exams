a=a,px=px)\nreturn(lis)}","metadata":{"execution":{"iopub.status.busy":"2024-07-07T10:37:06.723606Z","iopub.execute_input":"2024-07-07T10:37:06.725314Z","iopub.status.idle":"2024-07-07T10:37:06.740968Z"},"trusted":true},"execution_count":4,"outputs":[]},{"cell_type":"markdown","source":"**function for interpolation of points by Newton Forward Method**","metadata":{}},{"cell_type":"code","source":"polyf<-function(xi,yi){\nli<-c()\nL<-length(xi)\nx0<-xi[1]\nh<-xi[2]-xi[1]\nx<-xi[L]\nk<-1\nyy<-yi\nd<-rep(0,5)\na<-rep(0,5)\ndd<-c()\nd[k]<-yy[1]\nwhile (length(yy)>1){\nfor (i in 1:(length(yy)-1)){\ndd<-cbind(dd,yy[i+1]-yy[i])\n}\nli<-c(li,dd)\nk<-k+1\nd[k]<-dd[1]\nyy<-dd\ndd<-c()\n}\nvalu<-c(li[1],li[2],li[3],li[4],li[5],li[6],li[7],li[8],li[9],li[10])\nv<-d\na[1]=v[5]/(24*h^4)\na[2]=(v[4]/(6*h^3))-((4*v[5])/(24*h^4))*x0-((6*v[5])/(24*h^4))*h\na[3]=(v[3]/(2*h^2))-((3*v[4])/(6*h^3))*x0-((3*h*v[4])/(6*h^3))+((6*v[5])/(24*h^4))*x0^2+((18*v[5])/(24*h^4))*x0*h+((11*v[5])/(24*h^4))*h^2\na[4]=(v[2]/h)-(v[3]/(h^2))*x0-(v[3]/(2*h^2))*h+((2*v[4])/(6*h^3))*x0^2+((6*v[4])/(6*h^3))*h*x0+(v[4]/(6*h^3))*x0^2+((2*v[4])/(6*h^3))*h^2-((22*v[5])/(24*h^4))*x0*h^2-((18*v[5])/(24*h^4))*h*x0^2-((4*v[5])/(24*h^4))*x0^3-((6*v[5])/(24*h^4))*h^3\na[5]=v[1]-(v[2]/h)*x0+(v[3]/(2*h^2))*x0^2+(v[3]/(2*h^2))*h*x0+((v[4]/(6*h^3))*(-3*h*x0^2-2*h^2*x0-x0^3))+((v[5]/(24*h^4))*(11*x0^2*h^2+6*h*x0^3+6*x0*h^3+x0^4))\npx<-a[1]*x^4+a[2]*x^3+a[3]*x^2+a[4]*x+a[5]\nlis<-list(d=valu,a=a,px=px)\nreturn(lis)}","metadata":{"execution":{"iopub.status.busy":"2024-07-07T10:43:09.540514Z","iopub.execute_input":"2024-07-07T10:43:09.542240Z","iopub.status.idle":"2024-07-07T10:43:09.557920Z"},"trusted":true},"execution_count":7,"outputs":[]},{"cell_type":"markdown","source":"**function for interpolation of points by Newton Backward Method**","metadata":{}},{"cell_type":"code","source":"polyb<-function(xi,yi){\nli<-c()\nL<-length(xi)\nx0<-xi[L]\nh<-xi[2]-xi[1]\nx<-xi[L]\nk<-1\nyy<-yi\nd<-rep(0,5)\na<-rep(0,5)\ndd<-c()\nd[k]<-yy[L]\nwhile (length(yy)>1){\nfor (i in 1:(length(yy)-1)){\ndd<-cbind(dd,yy[i+1]-yy[i])\n}\nli<-c(li,dd)\nk<-k+1\nLk<-length(dd)\nd[k]<-dd[Lk]\nyy<-dd\ndd<-c()\n}\nvalu<-c(li[1],li[2],li[3],li[4],li[5],li[6],li[7],li[8],li[9],li[10])\nv<-d\na[1]=v[5]/(24*h^4)\na[2]=((v[4]/(6*h^3))-((4*v[5])/(24*h^4))*x0+((6*v[5])/(24*h^4))*h)\na[3]=(v[3]/(2*h^2))-((3*v[4])/(6*h^3))*x0+((3*v[4])/(6*h^3))*h+((6*v[5])/(24*h^4))*x0^2-((18*v[5])/(24*h^4))*x0*h+((11*v[5])/(24*h^4))*h^2\na[4]=(v[2]/h)-(v[3]/h^2)*x0+(v[3]/(2*h^2))*h+((2*v[4])/(6*h^3))*x0^2-((6*v[4])/(6*h^3))*h*x0+(v[4]/(6*h^3))*x0^2+((2*v[4])/(6*h^3))*h^2-((22*v[5])/(24*h^4))*x0*h^2+((18*v[5])/(24*h^4))*h*x0^2-((4*v[5])/(24*h^4))*x0^3+((6*v[5])/(24*h^4))*h^3\na[5]=v[1]-(v[2]/h)*x0+(v[3]/(2*h^2))*x0^2-(v[3]/(2*h^2))*h*x0+((v[4]/(6*h^3))*(+3*h*x0^2-2*h^2*x0-x0^3))+((v[5]/(24*h^4))*(11*x0^2*h^2-6*h*x0^3-6*x0*h^3+x0^4));\npx<-a[1]*x^4+a[2]*x^3+a[3]*x^2+a[4]*x+a[5]\nlis<-list(d=valu,a=a,px=px)\nreturn(lis)}","metadata":{"execution":{"iopub.status.busy":"2024-07-07T10:43:02.836880Z","iopub.execute_input":"2024-07-07T10:43:02.838539Z","iopub.status.idle":"2024-07-07T10:43:02.853440Z"},"trusted":true},"execution_count":6,"outputs":[]},{"cell_type":"markdown","source":"**function for interpolation of points by Newton for divided differences Method**","metadata":{}},{"cell_type":"code","source":"polyd<-function(xi,yi){\nli<-c()\nL<-length(xi)\ny<-xi\nx<-xi[5]\nk<-1\nd<-rep(0,5)\na<-rep(0,5)\ndd<-c()\nyy<-yi\nd[k]<-yy[1]\nwhile (length(yy)>1){\nfor (i in 1:(length(yy)-1)){\ndd<-cbind(dd,(yy[i+1]-yy[i])/(y[i+k]-y[i]))\n}\nli<-c(li,dd)\nk<-k+1\nd[k]<-dd[1]\nyy<-dd\ndd<-c()\n}\nvalu<-c(li[1],li[2],li[3],li[4],li[5],li[6],li[7],li[8],li[9],li[10])\nv<-d\na[1]<-v[5]\na[2]<-v[4]-(v[5]*y[1])-(v[5]*y[2])-(v[5]*y[3])-(v[5]*y[4])\na[3]<-v[3]-(v[4]*y[1])-(v[4]*y[2])-(v[4]*y[3])+(v[5]*y[2]*y[1])+(v[5]*y[3]*y[1])+(v[5]*y[3]*y[2])+(v[5]*y[4]*y[1])+(v[5]*y[4]*y[2])+(v[5]*y[4]*y[3])\na[4]<-v[2]-(v[3]*y[1])-(v[3]*y[2])+(v[4]*y[2]*y[1])+(v[4]*y[3]*y[1])